{
  "name": "guia_turistico",
  "version": "0.7.0-alpha",
  "description": "Tourist guide web application built on top of guia.js library",
  "type": "module",
  "main": "src/app.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:verbose": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose",
    "validate": "node -c src/guia.js && node -c src/guia_ibge.js",
    "test:all": "npm run validate && npm test",
    "lint": "eslint src/**/*.js __tests__/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js __tests__/**/*.js"
  },
  "keywords": [
    "tourist-guide",
    "geolocation",
    "brazil",
    "spa",
    "guia-js"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0 <26.0.0",
    "npm": ">=10.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/*.spec.js",
      "!node_modules/**",
      "!coverage/**"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 68,
        "branches": 73,
        "functions": 57,
        "lines": 68
      }
    }
  },
  "devDependencies": {
    "eslint": "^9.39.2",
    "jest": "^30.1.3",
    "jsdom": "^27.3.0"
  },
  "dependencies": {
    "guia.js": "github:mpbarbosa/guia_js",
    "ibira.js": "github:mpbarbosa/ibira.js"
  }
}
