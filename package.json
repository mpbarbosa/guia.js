{
  "name": "guia_js",
  "version": "0.5.0-alpha",
  "description": "Geolocation web application for Brazilian addresses",
  "type": "module",
  "main": "src/guia.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:verbose": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose",
    "validate": "node -c src/guia.js && node -c src/guia_ibge.js",
    "test:all": "npm run validate && npm test"
  },
  "keywords": ["geolocation", "brazil", "maps", "geocoding"],
  "author": "",
  "license": "ISC",
  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "collectCoverageFrom": [
      "src/*.js",
      "!node_modules/**"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/*.test.js"
    ]
  },
  "devDependencies": {
    "jest": "^30.1.3"
  }
}
